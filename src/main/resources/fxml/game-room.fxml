<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" minHeight="600.0" minWidth="600.0" style="-fx-background-color: #fff;"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Game rooms">
            <VBox.margin>
                <Insets bottom="20.0"/>
            </VBox.margin>
            <font>
                <Font name="Arial Bold" size="18.0"/>
            </font>
        </Text>
        <ListView maxWidth="400.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0"/>
        <Button fx:id="logout" mnemonicParsing="false" style="-fx-background-color: #fff; -fx-padding: 10 30;"
                text="Logout"
                textAlignment="CENTER" textFill="RED">
            <VBox.margin>
                <Insets top="40.0"/>
            </VBox.margin>
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
    </children>
</VBox>
